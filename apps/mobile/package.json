{
  "name": "@marketplace/mobile",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "dev": "expo start",
    "dev:clear": "expo start --clear",
    "kill-ports": "lsof -ti:8081,19000,19001 | xargs kill -9 || true",
    "restart": "npm run kill-ports && npm run dev:clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "lint": "eslint .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@marketplace/shared": "workspace:*",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "@react-native-community/datetimepicker": "^8.0.0",
    "@tanstack/react-query": "^5.17.0",
    "axios": "^1.6.5",
    "date-fns": "^4.1.0",
    "expo": "~54.0.0",
    "expo-av": "~16.0.8",
    "expo-blur": "~15.0.0",
    "expo-build-properties": "~1.0.0",
    "expo-constants": "~18.0.0",
    "expo-dev-client": "~5.1.0",
    "expo-device": "~7.0.0",
    "expo-haptics": "~14.0.0",
    "expo-image": "~3.0.0",
    "expo-image-picker": "~17.0.0",
    "expo-linear-gradient": "~14.0.1",
    "expo-linking": "~8.0.0",
    "expo-location": "~19.0.0",
    "expo-notifications": "~0.32.0",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.0",
    "expo-splash-screen": "~31.0.0",
    "expo-status-bar": "~3.0.0",
    "expo-web-browser": "~14.0.0",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-image-viewing": "^0.2.2",
    "react-native-map-supercluster": "^1.0.0",
    "react-native-maps": "1.20.1",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.2.0",
    "semver": "^7.6.3",
    "socket.io-client": "^4.7.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/react": "~19.1.10",
    "@types/react-native-vector-icons": "^6.4.18",
    "eslint": "^9.0.0",
    "typescript": "^5.3.3"
  }
}
